<h1> Genre name: <%= @genre.name %> </h1>
<h2> Genre ID: <%= @genre.id %> </h2>

<h2>Artists in this genre</h2>

<% @genre.artists.each do |artist| %>
  <p><a href="/artists/<%= artist.slug %>"> <%= artist.name %></a></p>
<% end %>


<h2>Songs in this genre</h2>

<% @genre.songs.each do |song| %>
  <li><a href="/songs/<%= song.slug %>"> <%= song.name %></a></li>
<% end %>
